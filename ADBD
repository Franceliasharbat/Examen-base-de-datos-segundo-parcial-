/*TABLA DE  PROFESORES*/
/*Se crea la tabla principal de profesores con las restrcciones que se pidieron*/
create table profe_tesoem (
id number (4),
primary KEY (id),
Num_empleado number (9),
Nombre varchar2 (30)not null,
Apell_pa varchar2 (30),
Apell_ma varchar2 (30),
Carrera varchar2 (30),
Nombre_clase varchar (60),
grupo varchar (5)
);

select * from profe_tesoem;/*selecciona la tabla y muestra el contenido*/
delete from prof_tesoem;/*borra el contenido de la tabla*/
drop table profe_tesoem;/*borra toda la tabla por completo*/


/*Se crea la funcion donde se puede actualizar, modificar, se le agrega un nombre y dentro de los parentesis nos indica que en caso de que falte alguno de los datos, se va a poner en automatico cada uno de los dafault indicados*/
create or replace procedure agregar_maestros
(m_id in number default 0, m_Num_empleado in number default 0, m_nombre in varchar2 default 'nombre', m_Apell_pa in varchar2 default 'apellido_pa', 
 m_Apell_ma in varchar2 default 'apellido_ma', m_carrera in varchar2 default 'carrera', m_Nombre_clase in varchar2 default 'clase', m_grupo in varchar2 default 'grupo')
 as 
 begin 
 insert into profe_tesoem values (m_id,m_Num_empleado, m_nombre, m_Apell_pa, m_Apell_ma, m_carrera, m_Nombre_clase, m_grupo);
  /*Se esta indicando cuales son los atributos que se van a insertar de cada uno de los profesores*/
 end;
 
  /* se ejecutan todos los datos que se pidieron dentro de profesores, y se agregan  los diferentes profesores con sus diferentes carreras, a la tabla principal que es lo que la funcion va a realizar*/
