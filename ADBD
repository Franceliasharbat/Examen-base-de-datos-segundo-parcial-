/*Se crea tabla de alumnos metiendo las restricciones que se pidieron primary, not null, etc */
create table alum_tesoem (
id number (3),
primary KEY (id),
Matricula number (9) unique,
Nombre varchar2 (30) not null,
Apell_pa varchar2 (30),
Apell_ma varchar2 (30),
Carrera varchar2 (30),
Grupo varchar2 (5),
Semestre number (2),
Edificio varchar2 (1),
Promedio number (3)
check (Promedio <=100)
);

delete from alum_tesoem;/* para borrar el contenido de la tabla*/
select * from alum_tesoem;/* para seleccionar la tabla */
drop table alum_tesoem; /* es para borrar la tabla por completo*/


/*Se crea la funcion donde se puede actualizar, modificar, se le agrega un nombre y dentro de los parentesis nos indica que en caso de que falte alguno de los datos, se va a poner en automatico cada uno de los dafault indicados*/
 create or replace procedure agregar_alum
(m_id in number default 0,m_Matricula in number default 0, m_Nombre in varchar2 default 'nombre', m_Apell_pa in varchar2 default 'apellido_pa', m_Apell_ma in varchar2 default 'apellido_ma',
 m_Carrera in varchar2 default 'carrera', m_Grupo in varchar2 default '00000', m_Semestre in number default 1, m_Edificio in varchar2 default '#', 
 m_Promedio in number default 0)
 as 
 begin 
 insert into alum_tesoem values (m_id,m_Matricula, m_Nombre, m_Apell_pa, m_Apell_ma, m_Carrera, m_Grupo,m_Semestre, m_Edificio, m_promedio);
 /*Se esta indicando cuales son los atributos que se van a insertar de cada uno de los alumnos*/
 end;
 /* se ejecutan todos los datos que se pidieron dentro de alumnos, y se agregan  los 90 alumnos, a la tabla principal que es lo que la funcion va a realizar*/
