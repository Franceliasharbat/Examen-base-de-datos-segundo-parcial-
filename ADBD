 /*CREACION DE TABLAS POR CARRERA PARA LOS ALUMNOS*/
/*SISTEMAS*/
/*Se crea la tabla con los atributos*/
create table ISC_1(
id number (3),
 Matricula number (9),
 Nombre varchar2 (50),
 Apellidos varchar2 (50),
 Grupo varchar2 (4),
 Edificio varchar2 (1)
 );
 
 drop table ISC_1;
 delete ISC_1;
 
 /*Se crea la funcion agregandole un nombre, dentro de la cual se puede agregar o actualizar el procedimiento almacenado*/
create or replace procedure alum_ISC
as
begin
insert into ISC_1 (id, matricula, Nombre, Apellidos, Grupo, Edificio)/*  se insertaran en la tabla creada de sistemas los campos que se seleccionaron de la tabla principal */
select id, Matricula, Nombre, Apell_pa | | ' ' | |  Apell_ma, Grupo, Edificio  from alum_tesoem /* se van a seleccionar los campos que se mandaran a traer de la tabla principial alumnos, concatenando unicamente apellido materno y apellido paterno*/
where Carrera = 'ISC'; /*en donde unicamente llamara a los alumnos que tengan como carrera "ISC" que son todos los de sistemas */
end; /*finaliza la funcion*/

execute alum_ISC; /* es para ejecutar el procedimiento almacenado*/
select* from ISC_1;/* para seleccionar la tabla de sistemas en donde apareceran los 10 alumnos de sistemas unicamente*/ 
delete from ISC_1;/* se borra el contenido de la tabla*/

/*En cada una de las tablas de las carreras es practicamente lo mismo solo que cambia los nombres de tablas, procedimientos, y obviamente los datos de los alumnos al insertarlos dependiendo de la carrera*/

/*TICS*/
create table TIC(
id number (3),
 Matricula number (9),
 Nombre varchar2 (50),
 Apellidos varchar2 (50),
 Grupo varchar2 (4),
 Edificio varchar2 (1)
 );

create or replace procedure alum_TIC
as
begin
insert into TIC (id, matricula, Nombre,Apellidos, Grupo, Edificio)
select id, Matricula,Nombre, Apell_pa | | ' ' | |  Apell_ma, Grupo, Edificio  from alum_tesoem 
where Carrera = 'TICS';
end;

execute alum_TIC;
select* from TIC;
delete from TIC;

/* Contabilidad*/
create table CONT(
id number (3),
 Matricula number (9),
 Nombre varchar2 (50),
 Apellidos varchar2 (50),
 Grupo varchar2 (4),
 Edificio varchar2 (1)
 );

drop table CONT;

create or replace procedure alum_CONT
as
begin
insert into CONT (id, matricula, Nombre,Apellidos, Grupo, Edificio)
select id, Matricula, Nombre, Apell_pa | | ' ' | |  Apell_ma, Grupo, Edificio  from alum_tesoem 
where Carrera = 'LC';
end;

execute alum_CONT;
select* from CONT;
delete from CONT;

/*Gastronomia*/
