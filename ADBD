 Nombre varchar2 (50),
 Apellidos varchar2 (50),
 Grupo varchar2 (8),
 Edificio varchar2 (1)
 );
 
create or replace procedure alumnos_AMB
as
begin
insert into AMB (id, matricula, Nombre,Apellidos, Grupo, Edificio)
select id, Matricula, Nombre, Apell_pa | | ' ' | |  Apell_ma, Grupo, Edificio  from alum_tesoem 
where Carrera = 'IA';
