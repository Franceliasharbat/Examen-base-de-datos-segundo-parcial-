/*Crear una funcion para determinar si son aptos a tener becas los alumnos del tesoem*/
/*Beca del 100*/
create table todo (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

/* se crea el procedicimiento almacenado */
create or replace procedure tododesc
as
begin
insert into todo (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)/*se insertar los datos de la tabla creada*/
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem /*posteriormente se van a seleccionar los datos de la tabla principal de alumnos */
where Promedio >=95; /* en donde todos los alumnos que tengan un promedio de 95 o mas  seran beneficiados por la beca del 100 */
end;/* fin del procedimiento*/

execute tododesc;/* ejecutar el procedimiento*/
select* from todo;/* selecciona la tabla y muestra todos los alumnos beneficiados por la beca*/
delete from todo;/* borra conbtenido de la tablas*/


/*En todas los procedimientos de las becas es lo mismo solo que unicamente cambia el  la condicion en donde cambia el promedio y de ahi se seleccionan los alumnos de la tabla principal que seran beneficiados de esa beca dependiendo de su promedio*/
/*Beca del 75*/
create table scdesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

create or replace procedure scde
as
begin
insert into scdesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio >=92 and Promedio <95;/*se seleccionan los alumnos en donde tengan un promedio mayor igual a 92, pero menor a  95*/
end;

execute scde;
select* from scdesc;
delete from scdesc;


/*Beca del 50*/
create table cidesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

create or replace procedure cide
as
begin
insert into cidesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio >=90 and Promedio <92; /*se seleccionan los alumnos que tengan promedio mayor igual a 90 pero menos de 92 teniendo una beca del 50%*/
end;

execute cide;
select* from cidesc;
delete from cidesc;

/*Beca del 35*/
create table tcdesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

create or replace procedure tcde
as
begin
insert into tcdesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio >=87 and Promedio <90; selecciona los alumnos
end;

execute tcde;
select* from tcdesc;
delete from tcdesc;

/*Beca del 25*/
create table vcdesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

drop table vcdesc;

create or replace procedure vcde
as
begin
insert into vcdesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio >=84 and Promedio <87; /*se seleccionan los alumnos donde tengan un promedieo mayor igual a 84 y menor a 87, teniendo uma beca del 25% de descuento*/
end;

execute vcde;
select* from vcdesc;
delete from vcdesc;

/*Beca del 10*/
create table ddesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

drop table ddesc;

create or replace procedure dde
as
begin
insert into ddesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio >=80 and Promedio <84; /*los alumnos con promedio mayor de igual a 80 y menos de 84 son seleccioandos para obtener una beca del 10%*/
end;

execute dde;
select* from ddesc;
delete from ddesc;

/*Sin beca*/
create table sndesc (
id number (5) unique,
Matricula number (10) not null,
Nombre varchar2 (70),
Apellidos varchar2 (70),
Carrera varchar2 (30),
Semestre number (3),
Promedio number (3)
);

drop table sndesc;

create or replace procedure snde
as
begin
insert into sndesc (id, matricula, Nombre,Apellidos, Carrera, Semestre, Promedio)
select id, Matricula, Nombre, Apell_pa ||' '||  Apell_ma, Carrera, Semestre, Promedio  from alum_tesoem 
where Promedio <80; /*los alumnos con un promedido menor a 80 no reciben algun tipo de beca*/
end;

execute snde;
select* from sndesc;
delete from sndesc;




