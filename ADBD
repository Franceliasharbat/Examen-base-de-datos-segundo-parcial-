insert into GA (id, matricula, Nombre,Apellidos, Grupo, Edificio)
select id, Matricula, Nombre, Apell_pa | | ' ' | |  Apell_ma, Grupo, Edificio  from alum_tesoem 
where Carrera = 'LG';
end;

execute alum_GA;
select* from GA;
delete from GA;
